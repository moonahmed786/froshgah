{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3||^8.0.2",
        "anandsiddharth/laravel-paytm-wallet": "^2.0.0",
        "barryvdh/laravel-dompdf": "^1.0",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^9.18.0",
        "intervention/image": "^2.5",
        "kingflamez/laravelrave": "^v4.2.0",
        "kkomelin/laravel-translatable-string-exporter": "^1.11",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.15",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.7",
        "mews/purifier": "^3.3",
        "mollie/laravel-mollie": "^2.0",
        "nwidart/laravel-modules": "^8.2",
        "paypal/rest-api-sdk-php": "*",
        "razorpay/razorpay": "2.*",
        "spatie/laravel-feed": "^4.2.0",
        "spatie/laravel-permission": "^5.5.5",
        "spatie/laravel-sitemap": "^6.2.0",
        "stripe/stripe-php": "^7.37",
        "unicodeveloper/laravel-paystack": "^1.0",
        "xgenious/paymentgateway": "^3.3.2",
        "xgenious/xgapiclient": "^1.6",
        "yajra/laravel-datatables-oracle": "~9.0",
        "ext-json": "*",
        "pusher/pusher-php-server": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "spatie/laravel-ignition": "^1.0",
        "laravel/ui": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Sharifur/paymentgateway.git"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
